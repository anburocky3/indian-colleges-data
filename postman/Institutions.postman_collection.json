        "url": {
          "raw": "{{base_url}}/api/institutions/search?state=Tamil Nadu&q=loyola&page=1&limit=20",
          "host": ["{{base_url}}"],
          "path": ["api", "institutions", "search"],
          "query": [
            { "key": "state", "value": "Tamil Nadu" },
            { "key": "q", "value": "loyola" },
