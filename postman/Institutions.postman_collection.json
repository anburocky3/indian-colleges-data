{
  "info": {
    "name": "Indian Colleges List API - AICTE Data",
    "_postman_id": "indian-colleges-aicte",
    "description": "Collection with requests for /api/institutions, state endpoints and programme details.",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "GET /api/institutions (offline default)",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/institutions",
          "host": ["{{base_url}}"],
          "path": ["api", "institutions"]
        }
      },
      "response": []
    },
    {
      "name": "GET /api/institutions (online example)",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/institutions?online=1&year=2025-2026&state=Tamil%20Nadu",
          "host": ["{{base_url}}"],
          "path": ["api", "institutions"],
          "query": [
            { "key": "online", "value": "1" },
            { "key": "year", "value": "2025-2026" },
            { "key": "state", "value": "Tamil Nadu" }
          ]
        }
      },
      "response": []
    },
    {
      "name": "GET /api/institutions/states (state list)",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/institutions/states",
          "host": ["{{base_url}}"],
          "path": ["api", "institutions", "states"]
        }
      },
      "response": []
    },
    {
      "name": "GET /api/institutions/states/:slug (example)",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/institutions/states/tamil-nadu",
          "host": ["{{base_url}}"],
          "path": ["api", "institutions", "states", "tamil-nadu"]
        }
      },
      "response": []
    },
    {
      "name": "GET /api/institutions/states/:state/:aicteid (example)",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/institutions/states/Tamil%20Nadu/1-44641241273",
          "host": ["{{base_url}}"],
          "path": [
            "api",
            "institutions",
            "states",
            "Tamil%20Nadu",
            "1-44641241273"
          ]
        }
      },
      "response": []
    },
